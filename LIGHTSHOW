int red = 2;  
int green =  3;    
int yellow = 4;  
int white =  5;

void setup()
{
  pinMode(red,OUTPUT);    
  pinMode(green,OUTPUT);
  pinMode(yellow,OUTPUT);    
  pinMode(white,OUTPUT);
}

void loop()
{
  digitalWrite(3, HIGH);
  digitalWrite(5, HIGH);
  delay(2800);
  digitalWrite(3, LOW);
  digitalWrite(5, LOW);
  delay(100);
  
  digitalWrite(3, HIGH);
  delay(300);
  digitalWrite(3, LOW);
  delay(100);
  
